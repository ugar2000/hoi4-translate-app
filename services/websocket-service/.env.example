# Redis connection URL
REDIS_URL=redis://localhost:6379

# Translation service URL
TRANSLATION_SERVICE_URL=http://localhost:3002

# Variable separator service URL
VARIABLE_SEPARATOR_URL=http://localhost:3003

# Microsoft Translator service URL
MICROSOFT_TRANSLATOR_SERVICE_URL=http://localhost:3004

# Port the WebSocket service listens on
PORT=3001
